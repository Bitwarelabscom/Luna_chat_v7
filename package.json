{
  "name": "luna-chat",
  "version": "1.0.0",
  "description": "Luna - AI Personal Assistant and Conversation Companion",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:prod": "tsc -p tsconfig.production.json",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:create": "tsx src/db/create-migration.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "chatbot",
    "ai",
    "assistant",
    "openai",
    "gpt",
    "luna"
  ],
  "author": "BitwareLabs",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "expr-eval": "^2.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "file-type": "^21.1.1",
    "groq-sdk": "^0.37.0",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.9.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "openai": "^4.68.0",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.0",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/expr-eval": "^1.1.2",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.42",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^20.14.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "tsx": "^4.15.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
